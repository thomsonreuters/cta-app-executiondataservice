{
  "extends": "airbnb",
  "ecmaFeatures": {
    "generators": true
  },
  "rules":{
    "strict":0,
    "generator-star-spacing": [2, {"before": false, "after": true}],
    "indent": [2, 2, {"SwitchCase": 1}],
    "linebreak-style": 0,
    "import/newline-after-import": 0,
    "max-len": ["error", { "ignoreTrailingComments": true }]
  },
  "env": {
    "mocha":true,
    "node":true,
    "es6":true
  }
}
